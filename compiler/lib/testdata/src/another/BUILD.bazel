load("@io_bazel_rules_go//go:def.bzl", "go_library")
load("@io_bazel_rules_go//proto:def.bzl", "go_proto_library")

proto_library(
    name = "another_proto",
    srcs = ["another.proto"],
    visibility = ["//visibility:public"],
)

go_proto_library(
    name = "another_go_proto",
    importpath = "github.com/protoconf/protoconf/compiler/lib/testdata/src/another",
    proto = ":another_proto",
    visibility = ["//visibility:public"],
)

go_library(
    name = "go_default_library",
    embed = [":another_go_proto"],
    importpath = "github.com/protoconf/protoconf/compiler/lib/testdata/src/another",
    visibility = ["//visibility:public"],
)
